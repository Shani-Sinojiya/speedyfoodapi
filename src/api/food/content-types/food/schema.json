{
  "kind": "collectionType",
  "collectionName": "foods",
  "info": {
    "singularName": "food",
    "pluralName": "foods",
    "displayName": "Food"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "code": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Gujarati",
        "Chinese",
        "Panjabi",
        "South Indian",
        "Other"
      ],
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Starter",
        "Main Course",
        "Dessert"
      ],
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "foods"
    }
  }
}
